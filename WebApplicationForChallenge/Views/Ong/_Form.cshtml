@model Ong

<div class="form-group">
    <label asp-for="Nome"></label>
    <input asp-for="Nome" class="form-control" />
</div>
<div class="form-group">
    <label asp-for="Ramo"></label>
    <select asp-for="Ramo" class="form-control" asp-items="@ViewBag.Ramo">
        <option>Selecione</option>
    </select>
</div>
<div class="form-group">
    <label asp-for="Porte"></label>
    <select asp-items="@Html.GetEnumSelectList<Porte>()"
            asp-for="Porte" class="form-control">
        <option>Selecione</option>
    </select>
</div>
<div class="form-group">
    <label asp-for="Endereco"></label>
    <input asp-for="Endereco" class="form-control" />
</div>
<div class="form-group">
    <label asp-for="Telefone"></label>
    <input asp-for="Telefone" class="form-control" />
</div>